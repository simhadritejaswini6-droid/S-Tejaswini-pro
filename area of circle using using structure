#include <stdio.h>
struct circle {
    float radius;
    float area;
};
int main() {
    struct circle c;
    float pi = 3.14159;
    printf("Enter the radius of the circle: ");
    scanf("%f", &c.radius);
    c.area = pi * c.radius * c.radius;
    printf("Area of the circle = %.2f\n", c.area);
    return 0;
}
