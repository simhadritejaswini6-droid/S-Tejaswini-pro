#include <stdio.h>
int main()
{
    int class_period;
    printf("Enter the class period number(1,2,3):");
    scanf("%d",&class_period);
    switch(class_period)
    {
        case 1:
        printf("1st period:Mathematics\n");
        printf("Timing:9:00am-10:00am\n");
        break;
        case 2:
            printf("2nd period:C programming\n");
            printf("Timing:10:00am-11:00am\n");
            break;
        case 3:
            printf("3rd priod:physics\n");
            printf("Timing:11:00am-12:00pm\n");
            break;
        default:
            printf("Invalid class period!");
            break;
    }
    return 0;
}


