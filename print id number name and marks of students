#include <stdio.h>
struct student{
    int id;
    char name[20];
    float marks;
};
    int main(){
    struct student s1={101,"priya",89.5};
    printf("ID:%d\n",s1.id);
    printf("Name:%s\n",s1.name);
    printf("Marks:%2f\n",s1.marks);
    struct student s2={102,"lasya",88.6};
        printf("ID:%d\n",s2.id);
    printf("Name:%s\n",s2.name);
    printf("Marks:%2f\n",s2.marks);
    struct student s3={103,"usha",87.4};
        printf("ID:%d\n",s3.id);
    printf("Name:%s\n",s3.name);
    printf("Marks:%2f\n",s3.marks);
    struct student s4={104,"teju",86.5};
        printf("ID:%d\n",s4.id);
    printf("Name:%s\n",s4.name);
    printf("Marks:%2f\n",s4.marks);
    return 0;
    }

