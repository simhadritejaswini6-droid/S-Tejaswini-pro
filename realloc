#include <stdio.h>
#include <stdlib.h>

int main()
{
    int *ptr;
    int i;
    ptr = (int *)malloc(3 * sizeof(int));

    if (ptr == NULL)
    {
        printf("Memory not allocated!\n");
        return 1;
    }
    for (i = 0; i < 3; i++)
    {
        ptr[i] = i + 1;
    }

    /* Reallocate memory for 5 integers */
    ptr = (int *)realloc(ptr, 5 * sizeof(int));

    if (ptr == NULL)
    {
        printf("Memory not reallocated!\n");
        return 1;
    }
    for (i = 3; i < 5; i++)
    {
        ptr[i] = i + 1;
  }
    for (i = 0; i < 5; i++)
    {
        printf("%d ", ptr[i]);
    }

    free(ptr);
return 0;
}
